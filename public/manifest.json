{
  "manifest_version": 3,
  "name": "Porto - Import Tweets to Bluesky",
  "version": "1.2.9",
  "description": "Easily import Tweets to Bluesky with few clicks",
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon.png"
    },
    "default_title": "Porto - Import Tweets to Bluesky"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["windows", "identity"],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon.png"
  }
}
